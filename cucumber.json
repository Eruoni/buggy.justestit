{
    "default": {
        "formatOptions": {
            "snippetInterface": "async-await"
        },
        "paths": [
            "src/test/features"
        ],
        "dryRun": true,
        "require": [
            "src/test/steps/*.ts",
            "src/hooks/hooks.ts"
        ],
        "requireModule": [
            "ts-node/register"
        ],
        "format": [
            "progress-bar",
            "html:test-results/cucumber-report.html",
            "json:test-results/cucumber-report.json",
            "rerun:@rerun.txt"
        ],
        "parallel": 1,
        "retry": 1
    },
    "rerun": {
        "formatOptions": {
            "snippetInterface": "async-await"
        },
        "dryRun": true,
        "require": [
            "src/test/steps/*.ts",
            "src/hooks/hooks.ts"
        ],
        "requireModule": [
            "ts-node/register"
        ],
        "format": [
            "progress-bar",
            "html:test-results/cucumber-report.html",
            "json:test-results/cucumber-report.json",
            "rerun:@rerun.txt"
        ],
        "parallel": 1
    }
}